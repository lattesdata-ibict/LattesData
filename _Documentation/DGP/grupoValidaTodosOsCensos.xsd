				<xs:annotation>
					<xs:documentation>Chave secundária definida para assegurar a
						integridade do atributo
						@SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA do elemento
						LINHA-DE-PESQUISA-RH com o atributo @SEQUENCIA-LINHA-DE-PESQUISA
						do elemento LINHA-DE-PESQUISA</xs:documentation>
				</xs:annotation>
			</xs:selector>
			<xs:field xpath="@SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA" />
		</xs:keyref>
		<xs:keyref name="RefSEQUENCIA-LINHA-DE-PESQUISA-PESQUISADOR"
			refer="ChaveSEQUENCIA-LINHA-DE-PESQUISA">
			<xs:selector
				xpath="PESQUISADORES/PESQUISADOR/LINHAS-DE-PESQUISA-RH/LINHA-DE-PESQUISA-RH">
				<xs:annotation>
					<xs:documentation>Chave secundária definida para assegurar a
						integridade do atributo
						@SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA do elemento
						LINHA-DE-PESQUISA-RH com o atributo @SEQUENCIA-LINHA-DE-PESQUISA
						do elemento LINHA-DE-PESQUISA</xs:documentation>
				</xs:annotation>
			</xs:selector>
			<xs:field xpath="@SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA" />
		</xs:keyref>
		<xs:keyref name="RefNOME-DA-LINHA-DE-PESQUISA-ESTUDANTE"
			refer="ChaveNOME-DA-LINHA-DE-PESQUISA">
			<xs:selector
				xpath="ESTUDANTES/ESTUDANTE/LINHAS-DE-PESQUISA-RH/LINHA-DE-PESQUISA-RH">
