	<xs:complexType name="TipoIDENTIFICACAO-DO-GRUPO">
		<xs:annotation>
			<xs:documentation>Tipo para o elemento de Identificação do Grupo
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LIDERES">
				<xs:complexType>
					<xs:all>
						<xs:element name="PRIMEIRO-LIDER" type="TipoLIDER"
							minOccurs="0" />
						<xs:element name="SEGUNDO-LIDER" type="TipoLIDER"
							minOccurs="0" />
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="ENDERECO-INSTITUCIONAL-DO-GRUPO" type="TipoENDERECO-INSTITUCIONAL-DO-GRUPO"
				minOccurs="0" />
			<xs:element name="REPERCUSSOES" type="string64000"
				minOccurs="0" />
			<xs:element name="REDES-DE-PESQUISA" minOccurs="0"
				maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="REDE-DE-PESQUISA" type="TipoREDE-DE-PESQUISA"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="NOME-DO-GRUPO" type="string200" use="optional" />
		<xs:attribute name="ANO-DE-CRIACAO" type="TipoANO" use="optional" />
		<xs:attribute name="GRANDE-AREA-PREDOMINANTE" type="xs:string"
			use="optional" />
		<xs:attribute name="AREA-PREDOMINANTE" type="string85"
			use="optional" />
		<xs:attribute name="NOME-DA-INSTITUICAO" type="string75"
			use="optional" />
		<xs:attribute name="SIGLA-DA-INSTITUICAO" type="string16"
			use="optional" />
		<xs:attribute name="UF-DA-INSTITUICAO" type="string2"
			use="optional" />
		<xs:attribute name="NOME-DO-PAIS-DA-INSTITUICAO" type="string20" />
		<xs:attribute name="SIGLA-DO-PAIS-DA-INSTITUICAO" type="string3" />
		<xs:attribute name="FLAG-AGENCIA-FOMENTO">
			<xs:simpleType>
				<xs:restriction base="xs:string" />
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FLAG-INSTITUICAO-DE-ENSINO">
			<xs:simpleType>
				<xs:restriction base="xs:string" />
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="NOME-DO-ORGAO" type="string75" use="optional" />
		<xs:attribute name="NOME-DA-UNIDADE" type="string75" use="optional" />
		<xs:attribute name="SITUACAO" type="string2" use="optional" />
		<xs:attribute name="CODIGO-AREA-PREDOMINANTE" type="string10" use="optional" />
		<xs:attribute name="NRO-ID-CNPQ-INSTITUICAO" type="nro-id-cnpq" use="optional" />
		
	</xs:complexType>
	<xs:complexType name="TipoLIDER">
		<xs:annotation>
			<xs:documentation>Tipo para o elemento Líder</xs:documentation>
		</xs:annotation>
		<xs:attribute name="NOME-COMPLETO" type="string60" use="required" />
		<xs:attribute name="NACIONALIDADE" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="B" />
					<xs:enumeration value="E" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="CPF" type="string11" use="optional" />
		<xs:attribute name="PAIS-DE-NASCIMENTO" type="string20"
			use="required" />
		<xs:attribute name="DATA-DE-NASCIMENTO" type="data" use="optional" />
		<xs:attribute name="FORMATO-DATA-DE-NASCIMENTO" use="optional"
			fixed="DDMMAAAA" />
		<xs:attribute name="NRO-ID-CNPQ" type="nro-id-cnpq" use="optional" />
	</xs:complexType>
	<xs:complexType name="TipoENDERECO-INSTITUCIONAL-DO-GRUPO">
		<xs:annotation>
			<xs:documentation>Tipo para o elemento Endereço Institucional do
				Grupo</xs:documentation>
		</xs:annotation>
		<xs:attribute name="LOGRADOURO-E-COMPLEMENTO" type="string60"
			use="required" />
		<xs:attribute name="BAIRRO" type="string25" use="optional" />
		<xs:attribute name="UF" type="string2" use="required" />
		<xs:attribute name="CIDADE" type="string45" use="required" />
		<xs:attribute name="CEP" type="string9" use="required" />
		<xs:attribute name="DDD" type="string5" use="optional" />
		<xs:attribute name="TELEFONE" type="string10" use="optional" />
		<xs:attribute name="RAMAL" type="string5" use="optional" />
		<xs:attribute name="FAX" type="string10" use="optional" />
		<xs:attribute name="E-MAIL" type="string70" use="required" />
		<xs:attribute name="HOME-PAGE" type="string255" use="optional" />
		<xs:attribute name="LOGRADOURO" type="string53" use="optional" />
		<xs:attribute name="COMPLEMENTO" type="string150" use="optional" />
		<xs:attribute name="NUMERO" type="string5" use="optional" />
		<xs:attribute name="LOCALIDADE" type="string255" use="optional" />
		<xs:attribute name="CAIXA-POSTAL" type="string10" use="optional" />
		<xs:attribute name="LATITUDE" type="string30" use="optional" />
		<xs:attribute name="LONGITUDE" type="string30" use="optional" />
		<xs:attribute name="DDDFAX" type="string5" use="optional" />
		<xs:attribute name="COD-MUNICIPIO-IBGE" type="string10"
			use="optional" />
		<xs:attribute name="COD-PAIS-ISO-A3" type="string3" use="optional" />


	</xs:complexType>
	<xs:complexType name="TipoRH">
		<xs:annotation>
			<xs:documentation>Tipo para os elementos Pesquisador e Estudante
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LINHAS-DE-PESQUISA-RH">
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element name="LINHA-DE-PESQUISA" minOccurs="0"
								maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="SEQUENCIA-LINHA-DE-PESQUISA"
										type="xs:int" use="required" />
									<xs:attribute name="SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA"
										type="xs:int" use="required" />
									<xs:attribute name="NOME-DA-LINHA-DE-PESQUISA"
										type="string255" use="required" />
																		
								</xs:complexType>
							</xs:element>
							<xs:element name="LINHA-DE-PESQUISA-RH" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="SEQUENCIA-LINHA-DE-PESQUISA"
										type="xs:int" use="required" />
									<xs:attribute name="SEQUENCIA-IDENTIFICADORA-DA-LINHA-DE-PESQUISA"
										type="xs:int" use="optional" />
									<xs:attribute name="NOME-DA-LINHA-DE-PESQUISA"
										type="string255" use="required" />									
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AREAS-DE-ATUACAO" type="TipoAREAS-DE-ATUACAO"
				minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="NOME-COMPLETO" type="string60" use="required" />
		<xs:attribute name="NACIONALIDADE" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="B" />
					<xs:enumeration value="E" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="CPF" type="string11" use="required" />
		<xs:attribute name="PAIS-DE-NASCIMENTO" type="string20"
			use="required" />
		<xs:attribute name="DATA-DE-NASCIMENTO" type="data" use="required" />
		<xs:attribute name="FORMATO-DATA-DE-NASCIMENTO" use="optional"
			fixed="DDMMAAAA" />
		<xs:attribute name="NRO-ID-CNPQ" use="optional" type="nro-id-cnpq" />
		<xs:attribute name="SEXO" type="sexo" />
	</xs:complexType>
	<xs:complexType name="TipoPesquisador">
		<xs:annotation>
			<xs:documentation>Tipo para o elemento Pesquisador</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TipoRH">
				<xs:attribute name="SEQUENCIA-PESQUISADOR" type="xs:int"
					use="optional" />
				<xs:attribute name="BOLSA-CATEGORIA" type="string200" />
				<xs:attribute name="BOLSA-NIVEL" type="string50" />
				<xs:attribute name="TITULACAO-MAXIMA" type="nivel-formacao" />
				<xs:attribute name="NOME-CITACOES" type="string2000"
					use="optional" />
				<xs:attribute name="EMAIL" type="string70" use="optional" />
				<xs:attribute name="HOME-PAGE" type="string250" use="optional" />
				<xs:attribute name="DATA-ENVIO-CV" type="data" use="optional" />
				<xs:attribute name="FORMATO-DATA-ENVIO-CV" use="optional"
					fixed="DDMMAAAA" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TipoEstudante">
		<xs:annotation>
			<xs:documentation>Tipo para o elemento Estudante</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
