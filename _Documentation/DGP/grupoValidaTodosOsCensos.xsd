						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EGRESSOS" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EGRESSO" type="TipoEgresso"
								minOccurs="0" maxOccurs="unbounded" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="SISTEMA-ORIGEM-XML" type="string20"
