			<xs:attribute name="ESTRATIFICACAO" />
			<xs:attribute name="NRO-ID-GRUPO" />
		</xs:complexType>
		<xs:unique name="ChaveSEQUENCIA-LINHA-DE-PESQUISA">
			<xs:selector xpath="LINHAS-DE-PESQUISA/LINHA-DE-PESQUISA">
