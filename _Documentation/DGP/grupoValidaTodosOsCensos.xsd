				</xs:annotation>
			</xs:selector>
			<xs:field xpath="@SEQUENCIA-ESTUDANTE" />
		</xs:unique>
		<xs:unique name="ChaveSEQUENCIA-PESQUISADOR">
			<xs:selector xpath="PESQUISADORES/PESQUISADOR">
				<xs:annotation>
					<xs:documentation>Chave primária para o atributo
						@SEQUENCIA-PESQUISADOR do elemento PESQUISADOR</xs:documentation>
				</xs:annotation>
			</xs:selector>
			<xs:field xpath="@SEQUENCIA-PESQUISADOR" />
		</xs:unique>
		<xs:keyref name="RefSEQUENCIA-LINHA-DE-PESQUISA-ESTUDANTE"
			refer="ChaveSEQUENCIA-LINHA-DE-PESQUISA">
